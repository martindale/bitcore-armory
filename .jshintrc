{
    "bitwise": false,
    "browser": true,
    "camelcase": true,
    "curly": true,
    "devel": false,
    "eqeqeq": true,
    "esnext": false,
    "freeze": true,
    "immed": true,
    "indent": 2,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "quotmark": "single",
    "regexp": true,
    "smarttabs": false,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true,

    "maxparams": 4,
    "maxstatements": 15,
    "maxcomplexity": 6,
    "maxdepth": 3,
    "maxlen": 80,

    "predef": [
        "after",
        "afterEach",
        "before",
        "beforeEach",
        "define",
        "describe",
        "exports",
        "it",
        "module",
        "require"
    ]
}
